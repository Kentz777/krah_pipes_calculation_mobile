{
  "expo": {
    "name": "Krah Pipes Calculation",
    "slug": "krah_pipes_calculation_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "Krah",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/krah_logo_TM.png",
      "resizeMode": "native",
      "backgroundColor": "#F1BD15"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
